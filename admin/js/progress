<script>
function progress(i)
{
	for (j=1;j<=i;j++)
	{
		e = document.getElementById("td"+j);
		if (e!=null)
			e.style.backgroundColor="#0000ff";
		//e.style.height='50px';
	}
	ee = document.getElementById("prog");
	if (i>100)
		pro=100;
	else
		pro=i;
	ee.innerHTML = pro+"%";
}
</script>